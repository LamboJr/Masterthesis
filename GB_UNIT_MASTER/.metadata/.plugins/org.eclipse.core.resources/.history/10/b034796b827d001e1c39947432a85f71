#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#include "platform.h"
#include "xil_printf.h"
#include "xparameters.h"

#include "structs"


void print_trainer_data(u32 *Pokemonbuffer,struct Pokemon Pokemonstructure){
	u32 i;
	for (i = 0; i < 100; i++) {
		Pokemonbuffer[i] = Pokemonbuffer[i] >> 16;
	}


}
