#include <stdio.h>
#include <stdlib.h>
#include <stdint.h>

#include "platform.h"
#include "xil_printf.h"
#include "xparameters.h"

#include "structs.h"
#include "constant.h"

#define FrameMask 0xFFFF0000
void decode_Pokemon_data(u16 *Pokemonbuffer){
	struct Pokemon Pokemonstruct;
	Pokemonstruct.personality =Pokemonbuffer[0];
	Pokemonstruct.personality = Pokemonstruct.personality << 16;
	Pokemonstruct.personality = Pokemonstruct.personality + Pokemonbuffer[1];

	Pokemonstruct.trainer_id = Pokemonbuffer[2];
	Pokemonstruct.trainer_id = Pokemonstruct.trainer_id << 16;
	Pokemonstruct.trainer_id = Pokemonstruct.trainer_id + Pokemonbuffer[3];
}
