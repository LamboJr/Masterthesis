#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int concat(u16 *Buffer1, u16 *Buffer2,u16 *ResultBuffer) {
	size_t Buffer1size = sizeof(Buffer1)/sizeof(u16);
	size_t Buffer2size = sizeof(Buffer2)/sizeof(u16);
	printf("Size of Buffer 1: %d and Buffer2 : %d",Buffer1size,Buffer2size);
	memcpy(ResultBuffer,Buffer1,sizeof(Buffer1));
	memcpy(ResultBuffer+Buffer1size, Buffer2, sizeof(Buffer2));

}


  a[0] = 1;
  a[1] = 2;
  b[0] = 3;
  b[1] = 4;
  b[2] = 5;
  int* c = malloc(5*sizeof(int));
  memcpy(c, a, 2*sizeof(int));
  memcpy(c+2, b, 3*sizeof(int));
  printf("%d\n", c[0]);
  printf("%d\n", c[1]);
  printf("%d\n", c[2]);
  printf("%d\n", c[3]);
  printf("%d\n", c[4]);
  return 0;
}
