#ifndef RINGBUFFER_HEADER
#define RINGBUFFER_HEADER

#define MAX_BUFFER_LENGTH 1024


struct BufferType{
	u16 Data[MAX_BUFFER_LENGTH];
	u16 WriteIndex;
	u16 ReadIndex;
};


void ResetControlBuffer();
void ResetDataBuffer();
void ResetBuffer(struct BufferType *Buffer);
u16 ReadDataBuffer();



#endif
