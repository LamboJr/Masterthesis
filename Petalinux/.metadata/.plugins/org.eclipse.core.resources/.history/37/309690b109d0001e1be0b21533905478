#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#include "../Includes/Pokemonbuffer.h"

void concat(u16 *Buffer1, u16 *Buffer2,u16 *ResultBuffer) {
	size_t Buffer1size = sizeof(Buffer1)/sizeof(u16);
	size_t Buffer2size = sizeof(Buffer2)/sizeof(u16);
	printf("Size of Buffer 1: %d and Buffer2 : %d",Buffer1size,Buffer2size);
	memcpy(ResultBuffer,Buffer1,sizeof(Buffer1));
	memcpy(ResultBuffer+Buffer1size, Buffer2, sizeof(Buffer2));

	for (int i = 0; i< (Buffer1size+Buffer2size);i++){
		printf("Resultbuffer[%d] = %d\n",i,ResultBuffer[i]);
	}
	return;
}

